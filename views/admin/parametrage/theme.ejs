<%- include('../../partials/admin/Header',{'username':locals.username,'activeDash':1}) %>
<div>
    <h1 class="text-black-50 mb-1">Change Theme Colors</h1>
</div>
<form method="POST">
    <div class="mb-3">

    </div>
    <div class="mb-3">
        <div>
            <h3 class="text-black-50 mb-1">Primary Color</h3>
        </div>
        <input name="primaryColor" type="color" class="form-control form-control-color" id="exampleColorInput" value="<%=locals.webParams['primary_color']%>" title="Choose your primary color">
    </div>
    <div class="mb-3">
        <div>
            <h3 class="text-black-50 mb-1">Secondary Color</h3>
        </div>
        <input name="secondaryColor" type="color" class="form-control form-control-color" id="exampleColorInput" value="<%=locals.webParams['secondary_color']%>" title="Choose your secondary color">
    </div>
    <div class="mb-3">
        <button type="submit" class="btn btn-dark">Submit</button>
    </div>
</form>
<%- include('../../partials/admin/Footer') %>